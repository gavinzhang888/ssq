<ol class="breadcrumb white">
    <li>
        <a href="/"><%= I18n.t :"page.static.home" %></a>
    </li>
    <li class="active"><%= I18n.t :"double_ball.modelname" %>预测
        <span ng-bind="double_balls.meta.total"></span>
        <%= I18n.t :"page.static.unit" %></li>
</ol>
<div class="action-bar">
    <div class="action-btns form-inline">
        <a href="javascript:void(0);" class="btn btn-default" ng-click="show_list=!show_list">
            <i class="fa fa-adjust"></i>
        </a>
        <a href="javascript:void(0);" ng-click="refreshPage()" class="btn btn-default">
            <i class="fa fa-refresh"></i>
        </a>
        <a href="/hreports" class="btn btn-default">
            历史记录
        </a>
        <button class="btn btn-default" data-toggle="collapse" data-target="#table-search" data-animate="" data-play="bounceInUp">
            <i class="fa fa-search"></i>
        </button>
        <div class="btn-group">
            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                <%= I18n.t :"page.static.more" %>
                <span class="caret"></span>
            </button>
            <ul class="dropdown-menu" role="menu">
                <li>
                    <a href="javascript:void(0);" ng-click="setPerpage(15)"><%= I18n.t :"page.static.per_page" %>
                        15
                        <%= I18n.t :"page.static.unit" %></a>
                </li>
                <li>
                    <a href="javascript:void(0);" ng-click="setPerpage(50)"><%= I18n.t :"page.static.per_page" %>
                        50
                        <%= I18n.t :"page.static.unit" %></a>
                </li>
                <li class="divider"></li>
                <li>
                    <a ng-href="/properties?t=double_ball" target="_blank"><%= I18n.t :"page.static.custom" %></a>
                </li>
                <li>
                    <a href="javascript:void(0);" ng-click="export()"><%= I18n.t :"page.static.export" %></a>
                </li>
                <li>
                    <a href="javascript:void(0);" print-div="#print-area"><%= I18n.t :"page.static.print" %></a>
                </li>
            </ul>
        </div>
    </div>

    <div class="willPaginate" data-params="willPaginateCollection" data-on-click="getPage" data-config="willPaginateConfig" ng-if="double_balls.meta.total > double_balls.meta.perpage"></div>
</div>
<div class="panel panel-default table-responsive" id="print-area" ng-init="getData()" ng-show="show_list">
    <div class="panel-body">
        <table class="table table-hover table-auto">
            <thead>
                <tr>
                    <th></th>
                    <th>序号</th>
                    <!-- <th sort-list-html data-filed="number" data-filed-name="<%= I18n.t :"double_ball.number" %>"></th> -->
                    <th><%= I18n.t :"double_ball.number" %></th>
                    <th><%= I18n.t :"double_ball.date" %></th>
                    <th><%= I18n.t :"double_ball.week_number" %></th>
                    <th ng-show="!show_reds && show_list" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.red_1" %></th>
                    <th ng-show="!show_reds && show_list" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.red_2" %></th>
                    <th ng-show="!show_reds && show_list" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.red_3" %></th>
                    <th ng-show="!show_reds && show_list" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.red_4" %></th>
                    <th ng-show="!show_reds && show_list" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.red_5" %></th>
                    <th ng-show="!show_reds && show_list" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.red_6" %></th>
                    <th style="text-align:center;" ng-show="show_reds && show_list" colspan="33" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.red_6" %></th>
                    <th ng-show="!show_reds && show_list" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.blue" %></th>
                    <th style="text-align:center;" ng-show="show_reds && show_list" colspan="16" ng-click="show_reds=!show_reds"><%= I18n.t :"double_ball.blue" %></th>
                    <th><%= I18n.t :"double_ball.odd" %></th>
                    <th><%= I18n.t :"double_ball.prime" %></th>
                    <th><%= I18n.t :"double_ball.red_total" %></th>
                    <th><%= I18n.t :"double_ball.total" %></th>
                    <th><%= I18n.t :"double_ball.large" %></th>
                    <th><%= I18n.t :"double_ball.created_at" %></th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="double_ball in double_balls" ng-show="!show_reds && show_list">
                    <td></td>
                    <td>{{$index + 1}}</td>
                    <td>
                        <a ng-href="/double_balls/{{double_ball.id}}">{{ double_ball.number }}</a>
                    </td>
                    <td>{{ double_ball.date }}</td>
                    <td>{{ double_ball.week_number }}</td>
                    <td>{{ double_ball.red_1 }}</td>
                    <td>{{ double_ball.red_2 }}</td>
                    <td>{{ double_ball.red_3 }}</td>
                    <td>{{ double_ball.red_4 }}</td>
                    <td>{{ double_ball.red_5 }}</td>
                    <td>{{ double_ball.red_6 }}</td>
                    <td>{{ double_ball.blue }}</td>
                    <td>{{ double_ball.odd }}</td>
                    <td>{{ double_ball.prime }}</td>
                    <td>{{ double_ball.red_total }}</td>
                    <td>{{ double_ball.total }}</td>
                    <td>{{ double_ball.large }}</td>
                    <td>{{ double_ball.created_at }}</td>
                </tr>
                <tr ng-repeat="double_ball in double_balls" ng-show="show_reds && show_list">
                    <td></td>
                    <td><input type="checkbox" ng-model="select_datas[double_ball.id]" ng-click="select_item(double_balls)"/></td>
                    <td>
                        <a ng-href="/double_balls/{{double_ball.id}}">{{ double_ball.number }}</a>
                    </td>
                    <td>{{ double_ball.date }}</td>
                    <td>{{ double_ball.week_number }}</td>
                    <td ng-repeat="all_red_number in all_red_numbers" class="red">
                        <span ng-show="double_ball.red_1 == all_red_number || double_ball.red_2 == all_red_number || double_ball.red_3 == all_red_number || double_ball.red_4 == all_red_number || double_ball.red_5 == all_red_number || double_ball.red_6 == all_red_number">
                            {{ all_red_number }}
                        </span>
                    </td>
                    <td ng-repeat="all_blue_number in all_blue_numbers" class="blue">
                        <span ng-show="double_ball.blue == all_blue_number">
                            {{ double_ball.blue }}
                        </span>
                    </td>
                    <td>{{ double_ball.odd }}</td>
                    <td>{{ double_ball.prime }}</td>
                    <td>{{ double_ball.red_total }}</td>
                    <td>{{ double_ball.total }}</td>
                    <td>{{ double_ball.large }}</td>
                    <td>{{ double_ball.created_at }}</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="panel panel-default table-responsive" id="print-area">
    <div class="panel-body">
        <h2 style="text-align:center;">和值走势图</h2>
        <canvas class="chart chart-line" chart-data="data" chart-labels="labels" chart-series="series" chart-legend="true"></canvas>
        <h2 style="text-align:center;">出球次数统计</h2>
        <canvas class="chart chart-line" chart-data="data19" chart-labels="labels2" chart-series="series19" chart-legend="true"></canvas>
        <h2 style="text-align:center;">奇数偶数分布图</h2>
        <canvas class="chart chart-line" chart-data="data2" chart-labels="labels" chart-series="series2" chart-legend="true"></canvas>
        <h2 style="text-align:center;">质数合数分布图</h2>
        <canvas class="chart chart-line" chart-data="data3" chart-labels="labels" chart-series="series3" chart-legend="true"></canvas>
        <h2 style="text-align:center;">大小号分布图(大号>=16)</h2>
        <canvas class="chart chart-line" chart-data="data6" chart-labels="labels" chart-series="series6" chart-legend="true"></canvas>
        <h2 style="text-align:center;">奇数质数分布图</h2>
        <canvas class="chart chart-line" chart-data="data4" chart-labels="labels" chart-series="series4" chart-legend="true"></canvas>
        <h2 style="text-align:center;">奇数合数分布图</h2>
        <canvas class="chart chart-line" chart-data="data7" chart-labels="labels" chart-series="series7" chart-legend="true"></canvas>
        <h2 style="text-align:center;">奇数大号分布图</h2>
        <canvas class="chart chart-line" chart-data="data8" chart-labels="labels" chart-series="series8" chart-legend="true"></canvas>
        <h2 style="text-align:center;">奇数小号分布图</h2>
        <canvas class="chart chart-line" chart-data="data9" chart-labels="labels" chart-series="series9" chart-legend="true"></canvas>
        <h2 style="text-align:center;">质数大号分布图</h2>
        <canvas class="chart chart-line" chart-data="data10" chart-labels="labels" chart-series="series10" chart-legend="true"></canvas>
        <h2 style="text-align:center;">质数小号分布图</h2>
        <canvas class="chart chart-line" chart-data="data11" chart-labels="labels" chart-series="series11" chart-legend="true"></canvas>
        <h2 style="text-align:center;">合数大号分布图</h2>
        <canvas class="chart chart-line" chart-data="data12" chart-labels="labels" chart-series="series12" chart-legend="true"></canvas>
        <h2 style="text-align:center;">合数小号分布图</h2>
        <canvas class="chart chart-line" chart-data="data13" chart-labels="labels" chart-series="series13" chart-legend="true"></canvas>
        <h2 style="text-align:center;">偶数质数分布图</h2>
        <canvas class="chart chart-line" chart-data="data14" chart-labels="labels" chart-series="series14" chart-legend="true"></canvas>
        <h2 style="text-align:center;">偶数合数分布图</h2>
        <canvas class="chart chart-line" chart-data="data15" chart-labels="labels" chart-series="series15" chart-legend="true"></canvas>
        <h2 style="text-align:center;">偶数大号分布图</h2>
        <canvas class="chart chart-line" chart-data="data16" chart-labels="labels" chart-series="series16" chart-legend="true"></canvas>
        <h2 style="text-align:center;">偶数小号分布图</h2>
        <canvas class="chart chart-line" chart-data="data17" chart-labels="labels" chart-series="series17" chart-legend="true"></canvas>
        <h2 style="text-align:center;">红首红尾分布图</h2>
        <canvas class="chart chart-line" chart-data="data18" chart-labels="labels" chart-series="series18" chart-legend="true"></canvas>
        <h2 style="text-align:center;">篮球走势图</h2>
        <canvas class="chart chart-line" chart-data="data5" chart-labels="labels" chart-series="series5" chart-legend="true"></canvas>
    </div>
</div>
<div class="action-bar">
<div class="willPaginate" data-params="willPaginateCollection" data-on-click="getPage" data-config="willPaginateConfig" ng-if="double_balls.meta.total > double_balls.meta.perpage"></div>
</div>
